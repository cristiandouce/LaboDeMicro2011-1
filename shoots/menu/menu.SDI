,,,       ;!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
,,,       ;!             Rutinas viejas de LCD solo para simulaciones
,,,       ;!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
,,,       
,,,       
,,,       ;*****************************************************************
,,,       ;*     Rutinas para trabajar con el LCD
,,,       ;*****************************************************************
,,,       
,,,       
,,,       LCD_init:
000244,9A39,,               sbi     DDRC,LCD_RS
000246,9A52,,               sbi DDRD,LCD_RW
000248,9A53,,               sbi     DDRD,LCD_E
,,,       
,,,                      ;hago un delay d 197ms 
00024A,D077,,               rcall   LCD_delay
,,,       
,,,                      ; configuro en modo 4 bits
00024C,E210,,               ldi     arg, 0x20
,,,       
,,,                      ;LCD is still in 8-BIT MODE while writing this command!!!
00024E,D00D,,               rcall LCD_command8
000250,D070,,               rcall LCD_wait
,,,       
,,,                      ;NOW: 2 lines, 5*7 font, 4-BIT MODE!
000252,E21C,,               ldi     arg, 0x2C
000254,D03B,,               rcall LCD_command
000256,D06D,,               rcall LCD_wait
,,,       
,,,                      ;now proceed as usual: Display on, cursor on, blinking
000258,E01F,,               ldi     arg, 0x0F
00025A,D038,,               rcall LCD_command
00025C,D06A,,               rcall LCD_wait
,,,       
,,,                      ;clear display, cursor -> home
00025E,E011,,               ldi     arg, 0x01
000260,D035,,               rcall LCD_command
000262,D067,,               rcall LCD_wait
,,,       
,,,                      ;auto-inc cursor
000264,E016,,               ldi     arg, 0x06
000266,D032,,               rcall LCD_command
000268,9508,,               ret
,,,       
,,,       ;*     Used for init (we need some 8-bit commands to switch to 4-bit mode!)
,,,       lcd_command8:
,,,                      ;we need to set the high nibble of DDRD while leaving
00026A,B10A,,               in      tmp, DDRD
,,,              
,,,                      ;the other bits untouched. Using tmp for that.
,,,                      ;set high nibble in tmp
00026C,6F00,,               sbr     tmp, 0b11110000
00026E,B90A,,               out     DDRD, tmp               ;write value to DDRD again-> d7-d4 son salidas
000270,B10B,,               in      tmp, PortD              ;then get the port value, aparentemente portd esta en cero x defecto
000272,700F,,               cbr     tmp, 0b11110000 ;borro the data bits 
000274,7F10,,               cbr     arg, 0b00001111 ;then clear the low nibble of the argument
,,,                      ;so that no control line bits are overwritten
000276,2B01,,               or      tmp, arg                ;then set the data bits (from the argument) in the
,,,                      ;Port value
,,,                      /*en argument tengo el argumento con el nibble d las variables d control en cero*/
,,,                      /*en tmp tngo los valores dl puerto d, con la parte d datos borrada*/
000278,B90B,,               out     PortD, tmp              ;and write the port value.
00027A,9A5B,,               sbi     PortD, LCD_E            ;now strobe E
00027C,0000,,               nop
00027E,0000,,               nop
000280,0000,,               nop
000282,985B,,               cbi     PortD, LCD_E
000284,B10A,,               in      tmp, DDRD               ;get DDRD to make the data lines input again
000286,700F,,               cbr     tmp, 0b11110000 ;clear data line direction bits
000288,B90A,,               out     DDRD, tmp               ;and write to DDRD
00028A,9508,,               ret
,,,       
,,,       lcd_putchar:
00028C,931F,,               push arg;save the argmuent (it's destroyed in between)
00028E,B10A,,               in      tmp, DDRD       ;get data direction bits
000290,6F00,,               sbr     tmp, 0b11110000 ;set the data lines to output
000292,B90A,,               out     DDRD, tmp               ;write value to DDRD
000294,B10B,,               in      tmp, PortD              ;then get the data from PortD
000296,7001,,               cbr     tmp, 0b11111110 ;clear ALL LCD lines (data and control!)
000298,7F10,,               cbr     arg, 0b00001111 ;we have to write the high nibble of our argument first
,,,                                              ;so mask off the low nibble
00029A,2B01,,               or      tmp, arg                ;now set the argument bits in the Port value
00029C,B90B,,               out     PortD, tmp              ;and write the port value
00029E,9A41,,               sbi     PortC, LCD_RS           ;now take RS high for LCD char data register access
0002A0,9A5B,,               sbi     PortD, LCD_E            ;strobe Enable
0002A2,0000,,               nop
0002A4,0000,,               nop
0002A6,0000,,               nop
0002A8,985B,,               cbi     PortD, LCD_E
0002AA,911F,,               pop     arg     ;restore the argument, we need the low nibble now...
0002AC,700F,,               cbr     tmp, 0b11110000 ;clear the data bits of our port value
0002AE,9512,,               swap    arg             ;we want to write the LOW nibble of the argument to
,,,                                              ;the LCD data lines, which are the HIGH port nibble!
0002B0,7F10,,               cbr     arg, 0b00001111 ;clear unused bits in argument
0002B2,2B01,,               or      tmp, arg                ;and set the required argument bits in the port value
0002B4,B90B,,               out     PortD, tmp              ;write data to port
0002B6,9A41,,               sbi     PortC, LCD_RS           ;again, set RS
0002B8,9A5B,,               sbi     PortD, LCD_E            ;strobe Enable
0002BA,0000,,               nop
0002BC,0000,,               nop
0002BE,0000,,               nop
0002C0,985B,,               cbi     PortD, LCD_E
0002C2,9841,,               cbi     PortC, LCD_RS
0002C4,B10A,,               in      tmp, DDRD
0002C6,700F,,               cbr     tmp, 0b11110000 ;data lines are input again
0002C8,B90A,,               out     DDRD, tmp
0002CA,9508,,               ret
,,,       
,,,       lcd_command:   ;same as LCD_putchar, but with RS low!
0002CC,931F,,               push    arg;guardo la instruccion
0002CE,B10A,,               in      tmp, DDRD;copio en tmp 00001110
0002D0,6F00,,               sbr     tmp, 0b11110000; hago un or, entonces pongo en 1 el nibble alto
0002D2,B90A,,               out     DDRD, tmp;pongo este valor en DDRD x lo tanto d7-d4 son salidas
0002D4,B10B,,               in      tmp, PortD;guardo el valor dl puerto
0002D6,7001,,               cbr     tmp, 0b11111110;hago una and con 01,borro todos los bits
0002D8,7F10,,               cbr     arg, 0b00001111;hago una and con F0, borro los 4 bits d control
0002DA,2B01,,               or      tmp, arg;hago una or me quedan los 4 bits altos d la instruccion y cero debajo
0002DC,B90B,,               out     PortD, tmp; lo cargo en el puerto
0002DE,9A5B,,               sbi     PortD, LCD_E
0002E0,0000,,               nop
0002E2,0000,,               nop
0002E4,0000,,               nop
0002E6,985B,,               cbi     PortD, LCD_E
0002E8,911F,,               pop     arg;saco el argumento d la pila
0002EA,700F,,               cbr     tmp, 0b11110000; hago una and con 0F, borro los dl nibble alto dejo los d control en cero
0002EC,9512,,               swap    arg;intercambio el nibble alto con el bajo
0002EE,7F10,,               cbr     arg, 0b00001111;hago una and con F0,borro el nibble inferior d argument
0002F0,2B01,,               or      tmp, arg;hago or con tmp
0002F2,B90B,,               out     PortD, tmp;cargo en el puerto 
0002F4,9A5B,,               sbi     PortD, LCD_E
0002F6,0000,,               nop
0002F8,0000,,               nop
0002FA,0000,,               nop
0002FC,985B,,               cbi     PortD, LCD_E
0002FE,B10A,,               in      tmp, DDRD
000300,700F,,               cbr     tmp, 0b11110000; vuelvo a poner el nibble alto en cero para q sea entrada.
000302,B90A,,               out     DDRD, tmp
000304,9508,,               ret
,,,       
,,,       LCD_getaddr:   ;works just like LCD_getchar, but with RS low, return.7 is the busy flag
000306,B10A,,               in      tmp, DDRD
000308,700F,,               andi tmp, 0b00001111;esto es al pedo
00030A,B90A,,               out     DDRD, tmp
00030C,9841,,               cbi     PortC, LCD_RS;pongo en cero para decir q es instruccion
00030E,9A5A,,               sbi     PortD, LCD_RW; en uno para leer
000310,9A5B,,               sbi     PortD, LCD_E; seteo enable
000312,0000,,               nop
000314,B109,,               in      tmp, PinD;guardo lo q estaba en el pin
000316,7F00,,               andi    tmp, 0b11110000;borro la parte d las variables d control
000318,2F60,,               mov     rtn, tmp;pongo en return ese valor
00031A,985B,,               cbi     PortD, LCD_E;pongo enable en cero
00031C,0000,,               nop
00031E,0000,,               nop;espero
000320,9A5B,,               sbi     PortD, LCD_E;pongo enable en uno
000322,0000,,               nop
000324,B109,,               in      tmp, PinD; guardo lo q esta en el pin, q es nibble bajo d la direccion, en realidad no lo necesito
000326,7F00,,               andi    tmp, 0b11110000;borro la parte d las variables d control
000328,9502,,               swap    tmp; pongo el nibble alto en el bajo
00032A,2B60,,               or      rtn, tmp;hago un or con return,tngo la dire completa
00032C,985B,,               cbi     PortD, LCD_E;pongo enable en cero
00032E,985A,,               cbi     PortD, LCD_RW; pongo rw en cero
000330,9508,,               ret
,,,       
,,,       ;*****************************************************************
,,,       ;*     Rutina de BUSY FLAG
,,,       ;*****************************************************************
,,,       LCD_wait:                              ;read address and busy flag until busy flag cleared
000332,DFE9,,               rcall   LCD_getaddr
000334,7860,,               andi    rtn, 0x80;hago un and con 10000000
000336,F7E9,,               brne    LCD_wait;sale d aca solo cuando el busy flag es cero
000338,9508,,               ret
,,,       
,,,       
,,,       ;*****************************************************************
,,,       ;*     Rutina de DELAY
,,,       ;*     Duracion de 197ms
,,,       ;*****************************************************************
,,,       LCD_delay:
00033A,2422,,               clr     r2;pongo r2 en cero
,,,       LCD_delay_outer:
00033C,2433,,               clr     r3;pongo r3 en cero
,,,       LCD_delay_inner:
00033E,943A,,               dec r3;decremento r3
000340,F7F1,,               brne LCD_delay_inner; sale del ciclo si r3 es igual a cero
000342,942A,,               dec     r2;cuando ya hizo 255us decremento r2
000344,F7D9,,               brne LCD_delay_outer;sale dl ciclo si r2 es igual a cero
000346,9508,,               ret
