,,,       ;*****************************************************************
,,,       ;*     Configuración de la comunicación SPI en MASTER
,,,       ;*****************************************************************
,,,       SPI_Minit:
,,,                      ;*      Set de SCK, MOSI y ~SS como salidas y MISO como
,,,                      ;*      entrada
0001CA,9A23,,               sbi     DDRB,pMOSI
0001CC,9A25,,               sbi     DDRB,pSCK
0001CE,9A22,,               sbi     DDRB,pSS
0001D0,9821,,               cbi     DDRB,pCSS
,,,                      ;*      Habilita comunicación SPI como MASTER a frecuencia
,,,                      ;*      de clock de f/16
0001D2,E701,,               ldi tmp, Cmaster
0001D4,BD0C,,               out SPCR, tmp
0001D6,B50D,,               in tmp, SPSR
0001D8,B50E,,               in tmp, SPDR
0001DA,9508,,               ret
,,,       ;******************************************************************
,,,       ;*     Enviar un String por SPI
,,,       ;******************************************************************
,,,       SPI_Sendstring:                                                                ;!
0001DC,E050,,               ldi     con,0x00                                                ;!
,,,       ssloop:                                                                                ;!
0001DE,E0E0,,               ldi Zl,0x00                                                     ;!
0001E0,E0F6,,               ldi     Zh,0x06                                                 ;!                      
0001E2,0FE5,,               add Zl,con                                                      ;!                                              
0001E4,9164,,               lpm     rtn,Z                                                   ;!                              
0001E6,BD6E,,               out     spdr,rtn                                                ;!              
0001E8,B57E,,               in      dta,spdr                                                ;!HAY QUE ADAPTARLA                     
0001EA,D008,,               rcall SPI_Wait                                          ;!                                              
0001EC,E0C0,,               ldi     Yl,low(string)                                  ;!                                              
0001EE,E0D1,,               ldi     Yh,high(string)                                 ;!                                              
0001F0,0FC5,,               add     Yl,con                                                  ;!                      
0001F2,8378,,               st      Y,dta                                                   ;!                      
0001F4,9553,,               inc con                                                         ;!              
0001F6,3062,,               cpi     rtn,fst                                                 ;!      
0001F8,F791,,               brne ssloop                                                     ;!                                      
0001FA,9508,,               ret                                                                     ;!
,,,       
,,,       ;*****************************************************************
,,,       ;*     Espera del fin de la recepción SPI
,,,       ;*****************************************************************
,,,       SPI_Wait:
,,,                      ;*      Espera del fin de la recepción
0001FC,B50D,,               in tmp, SPSR
0001FE,FF07,,               sbrs tmp, SPIF
000200,CFFD,,               rjmp SPI_Wait
000202,9508,,               ret
